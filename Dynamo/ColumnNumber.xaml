<Window x:Class="Dynamo.ColumnNumber"
        WindowStartupLocation="CenterOwner"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Dynamo"
      mc:Ignorable="d" Height="145" Width="180" ResizeMode="NoResize" ShowInTaskbar="False"
      >
    <Grid Margin="10,0,15,29">
        <Grid Background="white" HorizontalAlignment="Center" Height="105" Margin="0,14,0,0" VerticalAlignment="Top" Width="240" >

            <TextBox x:Name="cantColumn" TextWrapping="Wrap" Margin="100,0,110,70" Height="30" VerticalAlignment="Bottom"/>
            <Button x:Name="insert" Content="Insert" Margin="40,0,50,0" Height="20" Width="60" Click="insert_Click" />

        </Grid>
    </Grid>
</Window>
